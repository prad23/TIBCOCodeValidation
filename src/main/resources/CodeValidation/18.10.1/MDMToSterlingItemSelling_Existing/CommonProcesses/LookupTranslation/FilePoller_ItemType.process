<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:ns="http://www.tibco.com/schemas/Phase1BWTemplate/SharedResources/SchemaDefinitions/Common/Schema.xsd" xmlns:ns12="http://www.ultimosoft.com/CLEH/ExceptionHandler.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns3="http://www.tibco.com/pe/EngineTypes" xmlns:pfx2="http://www.tibco.com/schemas/Phase1BWTemplate/SharedResources/SchemaDefinitions/Common/Schema.xsd2" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx3="http://www.wsgc.com/schemas/common/ItemConcepts.xsd" xmlns:pfx4="http://www.wsgc.com/schemas/common/RMSLocations.xsd" xmlns:pfx5="http://www.wsgc.com/schemas/common/ConceptFilters.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx6="http://www.wsgc.com/schemas/common/CLEFlags.xsd" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:pfx7="http://www.wsgc.com/schemas/common/AppAuditSwitches.xsd" xmlns:pfx8="http://www.wsgc.com/OMS/BusinessProcesses/ItemMasterMDMToSterling/LookUp/SVCCardLookup.xsd" xmlns:pfx9="http://www.tibco.com/schemas/TIBCOSharedResources/SharedResources/XMLSchemas/Common/Schema.xsd3">
    <xsd:import namespace="http://www.tibco.com/schemas/Phase1BWTemplate/SharedResources/SchemaDefinitions/Common/Schema.xsd" schemaLocation="/SharedResources/SchemaDefinitions/Common/LookupTranslations.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/ItemConcepts.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/ItemConcepts.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/ConceptFilters.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/ConceptFilters.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/RMSLocations.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/CLEFlags.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/CLEFlags.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/AppAuditSwitches.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/AppAuditSwitches.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/OMS/BusinessProcesses/ItemMasterMDMToSterling/LookUp/SVCCardLookup.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/SVCCardLookup.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/TIBCOSharedResources/SharedResources/XMLSchemas/Common/Schema.xsd3" schemaLocation="/SharedResources/XMLSchemas/Common/ItemType.xsd"/>
    <pd:name>CommonProcesses/LookupTranslation/FilePoller_ItemType.process</pd:name>
    <pd:startName>File Poller</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="File Poller">
        <pd:type>com.tibco.plugin.file.FileEventSource</pd:type>
        <pd:resourceType>ae.activities.FileEventSourceResource</pd:resourceType>
        <pd:x>46</pd:x>
        <pd:y>64</pd:y>
        <config>
            <pollInterval>%%LookupTranslation/FilePollerIntervalInSec%%</pollInterval>
            <createEvent>false</createEvent>
            <modifyEvent>true</modifyEvent>
            <deleteEvent>false</deleteEvent>
            <mode>only-files</mode>
            <encoding>text</encoding>
            <sortby>File Name</sortby>
            <sortorder>descending</sortorder>
            <fileName>%%CommonFilePath/ItemTypeLookup%%</fileName>
            <encodingUsed>UTF8</encodingUsed>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>729</pd:endX>
    <pd:endY>69</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1200594477234</pd:targetNamespace>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>229</pd:x>
        <pd:y>66</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx9:ItemType"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$File-Poller/ns2:EventSourceOuputTextClass/fileContent/textContent"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SetVariable">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>362</pd:x>
        <pd:y>64</pd:y>
        <config>
            <variableConfig>/SharedResources/SharedVariables/SV-ItemTypeCode.sharedvariable</variableConfig>
            <showResult>false</showResult>
        </config>
        <pd:inputBindings>
            <pfx9:ItemType>
                <xsl:copy-of select="$Parse-XML/pfx9:ItemType/ancestor-or-self::*/namespace::node()"/>
                <xsl:copy-of select="$Parse-XML/pfx9:ItemType/@*"/>
                <xsl:copy-of select="$Parse-XML/pfx9:ItemType/node()"/>
            </pfx9:ItemType>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="LogChangeAsException">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>533</pd:x>
        <pd:y>67</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/CommonProcesses/CLEClientStub/SendExceptionRequest.process</processName>
        </config>
        <pd:inputBindings>
            <ns12:ExceptionHandlerRequest>
                <ns12:envCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ENV"/>
                </ns12:envCode>
                <ns12:appCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/CLEAppCode"/>
                </ns12:appCode>
                <ns12:interfaceCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/LookupTranslation/InterfaceCode"/>
                </ns12:interfaceCode>
                <ns12:hostName>
                    <xsl:value-of select="BW:getHostName()"/>
                </ns12:hostName>
                <ns12:timeStamp>
                    <xsl:value-of select="current-dateTime()"/>
                </ns12:timeStamp>
                <ns12:activityName>
                    <xsl:value-of select="&quot;CommonProcesses/LookupTranslation/FilePoller_ItemType/LogChangeAsException&quot;"/>
                </ns12:activityName>
                <ns12:message>
                    <xsl:value-of select="concat($File-Poller/ns2:EventSourceOuputTextClass/fileInfo/fullName, &quot; has been modified, updating corresponding shared variable&quot;)"/>
                </ns12:message>
                <ns12:payload>
                    <xsl:value-of select="$File-Poller/ns2:EventSourceOuputTextClass/fileContent/textContent"/>
                </ns12:payload>
                <ns12:ErrorLevel>
                    <ns12:errorCode>
                        <xsl:value-of select="&quot;TIB_COMMONLOOKUP_001&quot;"/>
                    </ns12:errorCode>
                </ns12:ErrorLevel>
                <ns12:TransactionDetails>
                    <ns12:transactionDomain>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/LookupTranslation/TransactionDomain"/>
                    </ns12:transactionDomain>
                    <ns12:transactionType>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/LookupTranslation/TransactionType"/>
                    </ns12:transactionType>
                    <ns12:transactionID>
                        <xsl:value-of select="$File-Poller/ns2:EventSourceOuputTextClass/fileInfo/fullName"/>
                    </ns12:transactionID>
                </ns12:TransactionDetails>
                <xsl:for-each xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$_processContext/ns3:ProcessContext">
                    <ns12:ProcessContextDetails>
                        <ns12:processId>
                            <xsl:value-of select="ProcessId"/>
                        </ns12:processId>
                        <ns12:projectName>
                            <xsl:value-of select="ProjectName"/>
                        </ns12:projectName>
                        <ns12:engineName>
                            <xsl:value-of select="EngineName"/>
                        </ns12:engineName>
                        <ns12:restartedFromCheckpoint>
                            <xsl:value-of select="RestartedFromCheckpoint"/>
                        </ns12:restartedFromCheckpoint>
                        <xsl:for-each select="TrackingInfo">
                            <ns12:trackingInfo>
                                <xsl:value-of select="."/>
                            </ns12:trackingInfo>
                        </xsl:for-each>
                    </ns12:ProcessContextDetails>
                </xsl:for-each>
            </ns12:ExceptionHandlerRequest>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>SetVariable</pd:from>
        <pd:to>LogChangeAsException</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>SetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>File Poller</pd:from>
        <pd:to>Parse XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogChangeAsException</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>