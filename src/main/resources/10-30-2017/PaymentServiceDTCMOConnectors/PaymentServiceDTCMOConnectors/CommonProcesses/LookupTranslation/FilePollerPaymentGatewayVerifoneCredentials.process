        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>590</pd:x>
        <pd:y>83</pd:y>
        <config>
            <spawn>false</spawn>
            <processName>/CommonProcesses/CLEClientStub/SendExceptionRequest.process</processName>
        </config>
        <pd:inputBindings>
            <pfx3:ExceptionHandlerRequest>
                <pfx3:envCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ENV"/>
                </pfx3:envCode>
                <pfx3:appCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/CLE/AppCode"/>
                </pfx3:appCode>
                <pfx3:interfaceCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/LookupTranslation/InterfaceCode"/>
