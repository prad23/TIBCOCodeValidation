                </pfx3:payload>
                <pfx3:ErrorLevel>
                    <pfx3:errorCode>
                        <xsl:value-of select="&quot;TIB_COMMONLOOKUP_001&quot;"/>
                    </pfx3:errorCode>
                </pfx3:ErrorLevel>
