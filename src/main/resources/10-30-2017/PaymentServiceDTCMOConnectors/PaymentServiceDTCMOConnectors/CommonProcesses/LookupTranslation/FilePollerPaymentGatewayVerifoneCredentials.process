                        <pfx9:ClientId>
                            <xsl:value-of select="ClientId"/>
                        </pfx9:ClientId>
                        <pfx9:DeviceKey>
                            <xsl:value-of select="DeviceKey"/>
                        </pfx9:DeviceKey>
                        <pfx9:DevType>
                            <xsl:value-of select="DevType"/>
                        </pfx9:DevType>
                        <pfx9:SerialNum>
                            <xsl:value-of select="SerialNum"/>
                        </pfx9:SerialNum>
                        <pfx9:Account>
                            <xsl:value-of select="Account"/>
                        </pfx9:Account>
                        <pfx9:Site>
                            <xsl:value-of select="Site"/>
                        </pfx9:Site>
                        <pfx9:Term>
                            <xsl:value-of select="Term"/>
                        </pfx9:Term>
                    </pfx9:PaymentGatewayVerifoneCredential>
                </xsl:for-each>
            </pfx9:PaymentGatewayVerifoneCredentials>
