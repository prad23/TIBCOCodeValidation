                        </DeviceKey>
                        <DevType>
                            <xsl:value-of select="pfx9:DevType"/>
                        </DevType>
                        <SerialNum>
                            <xsl:value-of select="pfx9:SerialNum"/>
                        </SerialNum>
                        <Account>
                            <xsl:value-of select="pfx9:Account"/>
                        </Account>
                        <ClientId>
                            <xsl:value-of select="pfx9:ClientId"/>
                        </ClientId>
                        <Site>
