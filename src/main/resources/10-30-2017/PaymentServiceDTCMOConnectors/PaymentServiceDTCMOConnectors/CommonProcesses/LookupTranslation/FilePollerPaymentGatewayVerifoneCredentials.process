    <xsd:import namespace="http://www.wsgc.com/schemas/common/PaymentGatewayCredentials.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/PaymentGatewayCredentials.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/PaymentGatewayVerifoneCredentials.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/PaymentGatewayVerifoneCredentials.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/PaymentGatewayADSCredentials.xsd"/>
    <pd:name>CommonProcesses/LookupTranslation/FilePollerPaymentGatewayVerifoneCredentials.process</pd:name>
    <pd:startName>File Poller</pd:startName>
