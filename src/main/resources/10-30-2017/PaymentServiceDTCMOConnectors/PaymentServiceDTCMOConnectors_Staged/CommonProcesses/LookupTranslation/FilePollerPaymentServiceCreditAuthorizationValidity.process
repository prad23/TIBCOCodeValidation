<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx="http://www.wsgc.com/schemas/common/AppAuditSwitches.xsd" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:pfx2="http://www.tibco.com/schemas/Phase1BWTemplate/SharedResources/SchemaDefinitions/Common/Schema.xsd2" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx3="http://www.ultimosoft.com/CLEH/ExceptionHandler.xsd" xmlns:pfx4="http://www.tibco.com/namespaces/tnt/plugins/mail" xmlns:pfx5="http://www.wsgc.com/schemas/common/PaymentGatewayCredentials.xsd" xmlns:pfx6="http://www.wsgc.com/schemas/common/ReturnsAppAuditSwitches.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:pfx7="http://www.wsgc.com/PGCredentials.xsd" xmlns:pfx8="http://www.wsgc.com/schemas/common/PaymentServiceCreditAuthorizationValidity.xsd">
    <xsd:import namespace="http://www.wsgc.com/schemas/common/ReturnsAppAuditSwitches.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/ReturnsAppAuditSwitches.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/AppAuditSwitches.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/AppAuditSwitches.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/PaymentGatewayCredentials.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/PaymentGatewayCredentials.xsd"/>
    <xsd:import namespace="http://www.wsgc.com/schemas/common/PaymentServiceCreditAuthorizationValidity.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/PaymentServiceCreditAuthorizationValidity.xsd"/>
    <pd:name>CommonProcesses/LookupTranslation/FilePollerPaymentServiceCreditAuthorizationValidity.process</pd:name>
    <pd:startName>File Poller</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="File Poller">
        <pd:type>com.tibco.plugin.file.FileEventSource</pd:type>
        <pd:resourceType>ae.activities.FileEventSourceResource</pd:resourceType>
        <pd:x>93</pd:x>
        <pd:y>83</pd:y>
        <config>
            <pollInterval>%%LookupTranslation/FilePollerIntervalInSec%%</pollInterval>
            <createEvent>false</createEvent>
            <modifyEvent>true</modifyEvent>
            <deleteEvent>false</deleteEvent>
            <mode>only-files</mode>
            <encoding>text</encoding>
            <sortby>File Name</sortby>
            <sortorder>descending</sortorder>
            <fileName>%%CommonFilePath/PaymentServiceCreditAuthorizationValidity%%</fileName>
            <encodingUsed>UTF8</encodingUsed>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>709</pd:endX>
    <pd:endY>83</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1200594563781</pd:targetNamespace>
    <pd:activity name="LogChangeAsException">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>590</pd:x>
        <pd:y>83</pd:y>
        <config>
            <spawn>false</spawn>
            <processName>/CommonProcesses/CLEClientStub/SendExceptionRequest.process</processName>
        </config>
        <pd:inputBindings>
            <pfx3:ExceptionHandlerRequest>
                <pfx3:envCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ENV"/>
                </pfx3:envCode>
                <pfx3:appCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/CLE/AppCode"/>
                </pfx3:appCode>
                <pfx3:interfaceCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/LookupTranslation/InterfaceCode"/>
                </pfx3:interfaceCode>
                <pfx3:hostName>
                    <xsl:value-of select="BW:getHostName()"/>
                </pfx3:hostName>
                <pfx3:timeStamp>
                    <xsl:value-of select="current-dateTime()"/>
                </pfx3:timeStamp>
                <pfx3:activityName>
                    <xsl:value-of select="&quot;CommonProcesses/LookupTranslation/FilePollerCAFNAppAuditSwitches/LogChangeAsException&quot;"/>
                </pfx3:activityName>
                <pfx3:message>
                    <xsl:value-of select="concat($File-Poller/ns:EventSourceOuputTextClass/fileInfo/fullName, &quot; has been modified, updating corresponding shared variable&quot;)"/>
                </pfx3:message>
                <pfx3:payload>
                    <xsl:value-of select="$File-Poller/ns:EventSourceOuputTextClass/fileContent/textContent"/>
                </pfx3:payload>
                <pfx3:ErrorLevel>
                    <pfx3:errorCode>
                        <xsl:value-of select="&quot;TIB_COMMONLOOKUP_001&quot;"/>
                    </pfx3:errorCode>
                </pfx3:ErrorLevel>
                <pfx3:TransactionDetails>
                    <pfx3:transactionDomain>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/LookupTranslation/TransactionDomain"/>
                    </pfx3:transactionDomain>
                    <pfx3:transactionType>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/LookupTranslation/TransactionType"/>
                    </pfx3:transactionType>
                    <pfx3:transactionID>
                        <xsl:value-of select="$File-Poller/ns:EventSourceOuputTextClass/fileInfo/fullName"/>
                    </pfx3:transactionID>
                </pfx3:TransactionDetails>
                <xsl:for-each select="$_processContext/ns2:ProcessContext">
                    <pfx3:ProcessContextDetails>
                        <pfx3:processId>
                            <xsl:value-of select="ProcessId"/>
                        </pfx3:processId>
                        <pfx3:projectName>
                            <xsl:value-of select="ProjectName"/>
                        </pfx3:projectName>
                        <pfx3:engineName>
                            <xsl:value-of select="EngineName"/>
                        </pfx3:engineName>
                        <pfx3:restartedFromCheckpoint>
                            <xsl:value-of select="RestartedFromCheckpoint"/>
                        </pfx3:restartedFromCheckpoint>
                        <xsl:for-each select="TrackingInfo">
                            <pfx3:trackingInfo>
                                <xsl:value-of select="."/>
                            </pfx3:trackingInfo>
                        </xsl:for-each>
                    </pfx3:ProcessContextDetails>
                </xsl:for-each>
            </pfx3:ExceptionHandlerRequest>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>230</pd:x>
        <pd:y>83</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx8:PaymentServiceCreditAuthorizationValidity"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$File-Poller/ns:EventSourceOuputTextClass/fileContent/textContent"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SetVariable">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>387</pd:x>
        <pd:y>83</pd:y>
        <config>
            <variableConfig>/SharedResources/SharedVariables/SV-PaymentServiceCreditAuthorizationValidity.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings>
            <pfx8:PaymentServiceCreditAuthorizationValidity>
                <xsl:for-each select="$Parse-XML/pfx8:PaymentServiceCreditAuthorizationValidity/pfx8:PaymentServiceCreditAuthorizationValidity">
                    <pfx8:PaymentServiceCreditAuthorizationValidity>
                        <pfx8:TenderSubType>
                            <xsl:value-of select="pfx8:TenderSubType"/>
                        </pfx8:TenderSubType>
                        <pfx8:AuthValidityInDays>
                            <xsl:value-of select="pfx8:AuthValidityInDays"/>
                        </pfx8:AuthValidityInDays>
                    </pfx8:PaymentServiceCreditAuthorizationValidity>
                </xsl:for-each>
            </pfx8:PaymentServiceCreditAuthorizationValidity>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>File Poller</pd:from>
        <pd:to>Parse XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogChangeAsException</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SetVariable</pd:from>
        <pd:to>LogChangeAsException</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>SetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>