    <pd:activity name="SendExceptionRequest">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>672</pd:x>
        <pd:y>231</pd:y>
        <config>
            <processName>/CommonProcesses/CLEClientStub/SendExceptionRequest.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="exists($_error_MapInputToXMLValidator/root/ns12:ExceptionHandlerRequest) and not (empty($_error_MapInputToXMLValidator/root/ns12:ExceptionHandlerRequest))">
                    <xsl:copy-of select="$_error_MapInputToXMLValidator/root/ns12:ExceptionHandlerRequest"/>
                </xsl:when>
                <xsl:when test="exists($_error_MapDTCMOControllerOutput/root/ns12:ExceptionHandlerRequest) and not (empty($_error_MapDTCMOControllerOutput/root/ns12:ExceptionHandlerRequest))">
                    <xsl:copy-of select="$_error_MapDTCMOControllerOutput/root/ns12:ExceptionHandlerRequest"/>
                </xsl:when>
                <xsl:otherwise>
                    <ns12:ExceptionHandlerRequest>
                        <ns12:envCode>
                            <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/ENV"/>
                        </ns12:envCode>
                        <ns12:appCode>
                            <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/CLE/AppCode"/>
                        </ns12:appCode>
                        <xsl:choose>
                            <xsl:when test="string-length($SetCLEParams/root/InterfaceCode)>0">
                                <ns12:interfaceCode>
                                    <xsl:value-of select="$SetCLEParams/root/InterfaceCode"/>
                                </ns12:interfaceCode>
                            </xsl:when>
                            <xsl:otherwise>
                                <ns12:interfaceCode>
                                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/LrcReturn/CLE/DTCInterfaceCode"/>
                                </ns12:interfaceCode>
                            </xsl:otherwise>
                        </xsl:choose>
                        <ns12:hostName>
                            <xsl:value-of select="BW:getHostName()"/>
                        </ns12:hostName>
                        <ns12:timeStamp>
                            <xsl:value-of select="current-dateTime()"/>
                        </ns12:timeStamp>
                        <ns12:message>
                            <xsl:value-of select="$_error/ns4:ErrorReport/Msg"/>
                        </ns12:message>
                        <xsl:choose xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                            <xsl:when test="string-length($LRCReturnMQSubscriber/ns13:ActivityOutput/Body) > 0">
                                <ns12:payload>
                                    <xsl:value-of select="$LRCReturnMQSubscriber/ns13:ActivityOutput/Body"/>
                                </ns12:payload>
                            </xsl:when>
                            <xsl:otherwise>
                                <ns12:payload>
                                    <xsl:value-of select="&quot;&quot;"/>
                                </ns12:payload>
                            </xsl:otherwise>
                        </xsl:choose>
                        <ns12:ErrorLevel>
                            <ns12:errorCode>
                                <xsl:value-of select="substring(concat($Map-Data/root/Type, $_globalVariables/ns2:GlobalVariables/PaymentService/Constants/ErrorCodeSeparator,&#xA;$Map-Data/root/Detail[1]),1, 100)"/>
                            </ns12:errorCode>
                        </ns12:ErrorLevel>
                        <ns12:TransactionDetails>
                            <ns12:transactionDomain>
                                <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/PaymentService/CLE/TransactionDomain"/>
                            </ns12:transactionDomain>
                            <ns12:transactionType>
                                <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/LrcReturn/CLE/TransactionType"/>
                            </ns12:transactionType>
                            <xsl:choose xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                                <xsl:when test="string-length($SetCLEParams/root/TransactionId) > 0">
                                    <ns12:transactionID>
                                        <xsl:value-of select="$SetCLEParams/root/TransactionId"/>
                                    </ns12:transactionID>
                                </xsl:when>
                                <xsl:otherwise>
                                    <ns12:transactionID>
                                        <xsl:value-of select="$TransactionID/TransactionID"/>
                                    </ns12:transactionID>
                                </xsl:otherwise>
                            </xsl:choose>
                        </ns12:TransactionDetails>
                        <xsl:for-each select="$_processContext/ns4:ProcessContext">
                            <ns12:ProcessContextDetails>
