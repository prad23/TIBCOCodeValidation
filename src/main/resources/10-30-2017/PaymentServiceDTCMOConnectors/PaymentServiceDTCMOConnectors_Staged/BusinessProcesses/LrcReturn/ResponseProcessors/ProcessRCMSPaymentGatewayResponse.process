    <xsd:import namespace="http://www.tibco.com/schemas/Payment Services/CreditAuthResponseSchema.xsd" schemaLocation="/CreditAuthResponse Schema.xsd"/>
    <xsd:import namespace="http://services.wsgc.com/2014/Payment" schemaLocation="/SharedResources/XMLSchemas/2014/Payment/lrc-return.xsd"/>
    <pd:name>BusinessProcesses/LrcReturn/ResponseProcessors/ProcessRCMSPaymentGatewayResponse.process</pd:name>
