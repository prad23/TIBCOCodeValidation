    <pd:activity name="AssignPymtGwayResponse">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>481</pd:x>
        <pd:y>377</pd:y>
        <config>
            <variableName>Process</variableName>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="exists($_error_ValidateRCMSPaymentGatewayResponse/root/Error) and not(empty($_error_ValidateRCMSPaymentGatewayResponse/root/Error))">
                    <xsl:copy-of select="$_error_ValidateRCMSPaymentGatewayResponse/root/Error"/>
                </xsl:when>
                <xsl:when test="exists($_error_MapRCMSPaymentServiceResponse/root/Error) and not(empty($_error_MapRCMSPaymentServiceResponse/root/Error))">
                    <xsl:copy-of select="$_error_ValidateRCMSPaymentGatewayResponse/root/Error"/>
                </xsl:when>
                <xsl:when test="$_error/ns1:ErrorReport/Class=$_globalVariables/ns:GlobalVariables/PaymentService/ValidationConstants/BIND_EXCEPTION_CLASS">
                    <Error>
                        <Type>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Type"/>
                        </Type>
                        <Detail>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Detail"/>
                        </Detail>
                        <ErrorMessage>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/ErrorMessage"/>
                        </ErrorMessage>
                    </Error>
                </xsl:when>
                <xsl:otherwise>
                    <Error>
                        <Type>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Type"/>
                        </Type>
                        <Detail>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Detail"/>
                        </Detail>
                        <ErrorMessage>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/ErrorMessage"/>
                        </ErrorMessage>
                    </Error>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Gen_PymtSvc_Map_Rsp">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>669</pd:x>
        <pd:y>375</pd:y>
        <config>
            <faultName>ErrorSchema</faultName>
        </config>
        <pd:inputBindings>
            <pfx6:ActivityInput>
                <xsl:if test="$_error/ns1:ErrorReport/Msg">
                    <message>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </message>
                </xsl:if>
                <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                    <messageCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </messageCode>
                </xsl:if>
                <data>
                    <root>
                        <xsl:choose>
                            <xsl:when test="exists($_error_ValidateRCMSPaymentGatewayResponse/root/ns12:ExceptionHandlerRequest) and not(empty($_error_ValidateRCMSPaymentGatewayResponse/root/ns12:ExceptionHandlerRequest))">
                                <xsl:copy-of select="$_error_ValidateRCMSPaymentGatewayResponse/root/ns12:ExceptionHandlerRequest"/>
                            </xsl:when>
                            <xsl:when test="exists($_error_MapRCMSPaymentServiceResponse/root/ns12:ExceptionHandlerRequest) and not(empty($_error_MapRCMSPaymentServiceResponse/root/ns12:ExceptionHandlerRequest))">
                                <xsl:copy-of select="$_error_MapRCMSPaymentServiceResponse/root/ns12:ExceptionHandlerRequest"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <ns12:ExceptionHandlerRequest>
                                    <ns12:envCode>
                                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/ENV"/>
                                    </ns12:envCode>
                                    <ns12:appCode>
                                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/CLE/AppCode"/>
                                    </ns12:appCode>
                                    <ns12:interfaceCode>
                                        <xsl:value-of select="$GetCLEParams/root/InterfaceCode"/>
                                    </ns12:interfaceCode>
                                    <ns12:hostName>
                                        <xsl:value-of select="BW:getHostName()"/>
                                    </ns12:hostName>
                                    <ns12:timeStamp>
                                        <xsl:value-of select="current-dateTime()"/>
                                    </ns12:timeStamp>
                                    <ns12:message>
                                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                                    </ns12:message>
                                    <ns12:payload>
                                        <xsl:value-of select="$Start/root/paymentgatewayresponse"/>
                                    </ns12:payload>
                                    <ns12:ErrorLevel>
                                        <ns12:errorCode>
                                            <xsl:value-of select="concat($Process/Error/Type, $_globalVariables/ns:GlobalVariables/PaymentService/Constants/ErrorCodeSeparator,&#xA;$Process/Error/Detail[1])"/>
                                        </ns12:errorCode>
                                    </ns12:ErrorLevel>
                                    <ns12:TransactionDetails>
                                        <ns12:transactionDomain>
                                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/CLE/TransactionDomain"/>
                                        </ns12:transactionDomain>
                                        <ns12:transactionType>
                                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/LrcReturn/CLE/TransactionType"/>
                                        </ns12:transactionType>
                                        <ns12:transactionID>
                                            <xsl:value-of select="$GetCLEParams/root/TransactionId"/>
                                        </ns12:transactionID>
                                    </ns12:TransactionDetails>
                                    <xsl:for-each select="$_error/ns1:ErrorReport">
                                        <ns12:ErrorReportType>
                                            <ns12:stackTrace>
                                                <xsl:value-of select="StackTrace"/>
                                            </ns12:stackTrace>
                                            <ns12:msg>
                                                <xsl:value-of select="Msg"/>
                                            </ns12:msg>
                                            <ns12:fullClass>
                                                <xsl:value-of select="FullClass"/>
                                            </ns12:fullClass>
                                            <ns12:class>
                                                <xsl:value-of select="Class"/>
                                            </ns12:class>
                                            <ns12:processStack>
                                                <xsl:value-of select="ProcessStack"/>
                                            </ns12:processStack>
                                            <xsl:if test="MsgCode">
                                                <ns12:msgCode>
                                                    <xsl:value-of select="MsgCode"/>
                                                </ns12:msgCode>
                                            </xsl:if>
                                            <ns12:data>
                                                <xsl:value-of select="Data/*"/>
                                            </ns12:data>
                                        </ns12:ErrorReportType>
                                    </xsl:for-each>
                                </ns12:ExceptionHandlerRequest>
                            </xsl:otherwise>
                        </xsl:choose>
                        <xsl:if test="string-length($Process/Error/Type)>0">
                            <Error>
                                <Type>
                                    <xsl:value-of select="$Process/Error/Type"/>
                                </Type>
                                <xsl:for-each select="$Process/Error/Detail">
                                    <Detail>
                                        <xsl:value-of select="."/>
                                    </Detail>
                                </xsl:for-each>
                                <ErrorMessage>
                                    <xsl:value-of select="$Process/Error/ErrorMessage"/>
                                </ErrorMessage>
                            </Error>
                        </xsl:if>
                    </root>
                </data>
            </pfx6:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ValidateRCMSPaymentGatewayResponse">
