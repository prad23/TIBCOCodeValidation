    <pd:endY>256</pd:endY>
    <pd:errorSchemas>
        <ErrorSchema>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element ref="ns12:ExceptionHandlerRequest" minOccurs="0"/>
                        <xsd:element name="Error" type="pfx5:ErrorType" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </ErrorSchema>
    </pd:errorSchemas>
