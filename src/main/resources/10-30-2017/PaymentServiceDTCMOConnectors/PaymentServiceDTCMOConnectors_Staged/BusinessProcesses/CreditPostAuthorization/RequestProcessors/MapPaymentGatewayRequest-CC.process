    <pd:activity name="MapPaymentGatewayRequest">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>523</pd:x>
        <pd:y>328</pd:y>
        <config>
            <element ref="pfx5:UGPREQUEST"/>
        </config>
        <pd:inputBindings>
            <xsl:variable xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="inchannel" select="$Start/pfx:CreditCardPostAuthorizationRequest/PaymentContext/Channel"/>
            <xsl:variable xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="inconcept" select="$Start/pfx:CreditCardPostAuthorizationRequest/PaymentContext/Concept"/>
            <pfx5:UGPREQUEST>
                <xsl:attribute name="VER">
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/PaymentService/PaymentGateway/VERIFONE/Version"/>
                </xsl:attribute>
                <FUNCTION_TYPE>
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/PaymentService/PaymentGateway/VERIFONE/FUNCTION_TYPE"/>
                </FUNCTION_TYPE>
                <PAYMENT_TYPE>
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/PaymentService/PaymentGateway/VERIFONE/CREDIT_PAYMENT_TYPE"/>
                </PAYMENT_TYPE>
                <COMMAND>
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/PostAuthorization/PaymentGateway/Command"/>
                </COMMAND>
                <ACCOUNT>
                    <xsl:value-of select="$PaymentGatewayVerifoneCredentialsParams/ns4:PaymentGatewayVerifoneCredentials/ns4:PaymentGatewayVerifoneCredential[$inconcept  = ns4:ConceptCode][ns4:ChannelCode=$inchannel]/ns4:Account"/>
                </ACCOUNT>
                <SITE>
                    <xsl:value-of select="$PaymentGatewayVerifoneCredentialsParams/ns4:PaymentGatewayVerifoneCredentials/ns4:PaymentGatewayVerifoneCredential[$inconcept  = ns4:ConceptCode][ns4:ChannelCode=$inchannel]/ns4:Site"/>
                </SITE>
                <TERM>
                    <xsl:value-of select="$PaymentGatewayVerifoneCredentialsParams/ns4:PaymentGatewayVerifoneCredentials/ns4:PaymentGatewayVerifoneCredential[$inconcept  = ns4:ConceptCode][ns4:ChannelCode=$inchannel]/ns4:Term"/>
                </TERM>
                <SERIAL_NUM>
                    <xsl:value-of select="$PaymentGatewayVerifoneCredentialsParams/ns4:PaymentGatewayVerifoneCredentials/ns4:PaymentGatewayVerifoneCredential[$inconcept  = ns4:ConceptCode][ns4:ChannelCode=$inchannel]/ns4:SerialNum"/>
                </SERIAL_NUM>
                <DEVTYPE>
                    <xsl:value-of select="$PaymentGatewayVerifoneCredentialsParams/ns4:PaymentGatewayVerifoneCredentials/ns4:PaymentGatewayVerifoneCredential[$inconcept  = ns4:ConceptCode][ns4:ChannelCode=$inchannel]/ns4:DevType"/>
                </DEVTYPE>
                <DEVICEKEY>
                    <xsl:value-of select="$PaymentGatewayVerifoneCredentialsParams/ns4:PaymentGatewayVerifoneCredentials/ns4:PaymentGatewayVerifoneCredential[$inconcept  = ns4:ConceptCode][ns4:ChannelCode=$inchannel]/ns4:DeviceKey"/>
                </DEVICEKEY>
                <TRANS_AMOUNT>
                    <xsl:value-of select="tib:string-round-fraction($Start/pfx:CreditCardPostAuthorizationRequest/CaptureAmount,$_globalVariables/ns2:GlobalVariables/PaymentService/Constants/AmountFractionalDigits)"/>
                </TRANS_AMOUNT>
                <AUTH_CODE>
                    <xsl:value-of select="$Start/pfx:CreditCardPostAuthorizationRequest/Authorization/Code"/>
                </AUTH_CODE>
                <xsl:if test="string-length($Start/pfx:CreditCardPostAuthorizationRequest/Tender/AccountNumber)>0">
                    <xsl:if test="$Start/pfx:CreditCardPostAuthorizationRequest/Tender/AccountNumber">
                        <ACCT_NUM>
                            <xsl:value-of select="$Start/pfx:CreditCardPostAuthorizationRequest/Tender/AccountNumber"/>
                        </ACCT_NUM>
                    </xsl:if>
                </xsl:if>
                <TKN_PROCESS>
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/PaymentService/PaymentGateway/VERIFONE/TOKEN_PROCESS"/>
                </TKN_PROCESS>
                <xsl:if test="(string-length($Start/pfx:CreditCardPostAuthorizationRequest/CardMetadata/CardExpiration/Year)>0) and&#xA;(string-length($Start/pfx:CreditCardPostAuthorizationRequest/CardMetadata/CardExpiration/Month)>0)">
                    <EXP_DATE>
                        <xsl:value-of select="concat(tib:pad-front($Start/pfx:CreditCardPostAuthorizationRequest/CardMetadata/CardExpiration/Month, 2, '0'),$Start/pfx:CreditCardPostAuthorizationRequest/CardMetadata/CardExpiration/Year)"/>
                    </EXP_DATE>
                </xsl:if>
                <INVOICE>
                    <xsl:value-of select="tib:right(concat($Start/pfx:CreditCardPostAuthorizationRequest/ConsumerTransactionId,$Start/pfx:CreditCardPostAuthorizationRequest/OrderContext/@omsOrderId),$_globalVariables/ns2:GlobalVariables/PaymentService/Constants/InvoiceLength)"/>
                </INVOICE>
                <xsl:if test="string-length($Start/pfx:CreditCardPostAuthorizationRequest/Tender/Token)>0">
                    <xsl:if test="$Start/pfx:CreditCardPostAuthorizationRequest/Tender/Token">
                        <TKN_PAYMENT>
                            <xsl:value-of select="$Start/pfx:CreditCardPostAuthorizationRequest/Tender/Token"/>
                        </TKN_PAYMENT>
                    </xsl:if>
                </xsl:if>
                <xsl:if test="$Start/pfx:CreditCardPostAuthorizationRequest/IncludedTaxAmount !=$Start/pfx:CreditCardPostAuthorizationRequest/CaptureAmount">
                    <xsl:if test="$Start/pfx:CreditCardPostAuthorizationRequest/IncludedTaxAmount">
                        <TAX_AMOUNT>
                            <xsl:value-of select="$Start/pfx:CreditCardPostAuthorizationRequest/IncludedTaxAmount"/>
                        </TAX_AMOUNT>
                    </xsl:if>
                </xsl:if>
                <TKN_RENEW>
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/PaymentService/PaymentGateway/VERIFONE/TKN_RENEW"/>
                </TKN_RENEW>
            </pfx5:UGPREQUEST>
        </pd:inputBindings>
    </pd:activity>
