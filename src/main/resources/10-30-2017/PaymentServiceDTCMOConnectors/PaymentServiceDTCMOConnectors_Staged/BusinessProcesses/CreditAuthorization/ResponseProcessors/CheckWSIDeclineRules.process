<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://ecommerce.wsgc.com/2013/Common" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.wsgc.com/schemas/common/PaymentServiceWSIDeclineRules.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns2="http://www.tibco.com/pe/EngineTypes">
    <xsd:import namespace="http://ecommerce.wsgc.com/2013/Common" schemaLocation="/SharedResources/XMLSchemas/2013/Common/base-types.xsd"/>
    <pd:name>BusinessProcesses/CreditAuthorization/ResponseProcessors/CheckWSIDeclineRules.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="AVSCode" type="xsd:string"/>
                    <xsd:element name="CVVCode" type="xsd:string"/>
                    <xsd:element name="Concept" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>90</pd:startX>
    <pd:startY>177</pd:startY>
    <pd:returnBindings>
        <root>
            <xsl:if test="string-length( $RuleCheckOutput/root/Rule) > 0">
                <Rule>
                    <xsl:value-of select="$RuleCheckOutput/root/Rule"/>
                </Rule>
            </xsl:if>
            <xsl:if test="string-length($RuleCheckOutput/root/WSIDeclineStatus) > 0">
                <WSIDeclineStatus>
                    <xsl:value-of select="$RuleCheckOutput/root/WSIDeclineStatus"/>
                </WSIDeclineStatus>
            </xsl:if>
            <xsl:copy-of select="$RuleCheckOutput/root/Error"/>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Rule" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WSIDeclineStatus" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Error" type="pfx:ErrorType" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>806</pd:endX>
    <pd:endY>173</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <RuleCheckOutput>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Rule" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="WSIDeclineStatus" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="Error" type="pfx:ErrorType" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </RuleCheckOutput>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1436308681321</pd:targetNamespace>
    <pd:activity name="GetVariable">
        <pd:type>com.tibco.pe.core.GetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.getSharedVariable</pd:resourceType>
        <pd:x>287</pd:x>
        <pd:y>178</pd:y>
        <config>
            <variableConfig>/SharedResources/SharedVariables/SV-PaymentServiceWSIDeclineRules.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="AssignAVSCVVCheck">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>519</pd:x>
        <pd:y>42</pd:y>
        <config>
            <variableName>RuleCheckOutput</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <Rule>
                    <xsl:value-of select="$GetVariable/ns:PaymentServiceWSIDeclineRules/ns:PaymentServiceWSIDeclineRule[   $Start/root/CVVCode   = ns:PGCVVCode   ][   $Start/root/AVSCode  = ns:PGAVSCode]/ns:Rule"/>
                </Rule>
                <WSIDeclineStatus>
                    <xsl:value-of select="$GetVariable/ns:PaymentServiceWSIDeclineRules/ns:PaymentServiceWSIDeclineRule[   $Start/root/AVSCode = ns:PGAVSCode][   $Start/root/CVVCode   = ns:PGCVVCode   ]/ns:WSIDeclineStatus"/>
                </WSIDeclineStatus>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="AssignCVVConceptCheck">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>524</pd:x>
        <pd:y>266</pd:y>
        <config>
            <variableName>RuleCheckOutput</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <Rule>
                    <xsl:value-of select="$GetVariable/ns:PaymentServiceWSIDeclineRules/ns:PaymentServiceWSIDeclineRule[   $Start/root/CVVCode   = ns:PGCVVCode   ][   $Start/root/Concept  = $GetVariable/ns:PaymentServiceWSIDeclineRules/ns:PaymentServiceWSIDeclineRule/ns:Concept]/ns:Rule"/>
                </Rule>
                <WSIDeclineStatus>
                    <xsl:value-of select="$GetVariable/ns:PaymentServiceWSIDeclineRules/ns:PaymentServiceWSIDeclineRule[   $Start/root/CVVCode   = ns:PGCVVCode   ][   $Start/root/Concept  = $GetVariable/ns:PaymentServiceWSIDeclineRules/ns:PaymentServiceWSIDeclineRule/ns:Concept]/ns:WSIDeclineStatus"/>
                </WSIDeclineStatus>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>567</pd:x>
        <pd:y>380</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Assign">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>805</pd:x>
        <pd:y>379</pd:y>
        <config>
            <variableName>RuleCheckOutput</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <Error>
                    <Type>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Type"/>
                    </Type>
                    <ErrorMessage>
                        <xsl:value-of select="$_error/ns2:ErrorReport/Msg"/>
                    </ErrorMessage>
                </Error>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="AssignAVSCVVCheck-1">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>529</pd:x>
        <pd:y>149</pd:y>
        <config>
            <variableName>RuleCheckOutput</variableName>
        </config>
        <pd:inputBindings>
            <root/>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetVariable</pd:from>
        <pd:to>AssignCVVConceptCheck</pd:to>
        <pd:xpathDescription>CVV and Concept Check</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>contains($_globalVariables/ns1:GlobalVariables/PaymentService/WSIDeclineRules/Rule3Concepts,  $Start/root/Concept)  and 
string-length($Start/root/CVVCode) &gt; 0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>GetVariable</pd:from>
        <pd:to>AssignAVSCVVCheck</pd:to>
        <pd:xpathDescription>AVSCode and CVSCode Checked</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>string-length($Start/root/AVSCode) &gt; 0 and 
string-length($Start/root/CVVCode) &gt; 0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>AssignAVSCVVCheck</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>AssignCVVConceptCheck</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Assign</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetVariable</pd:from>
        <pd:to>AssignAVSCVVCheck-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>AssignAVSCVVCheck-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>