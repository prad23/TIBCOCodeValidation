    <pd:activity name="MapPaymentServiceResponse">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>860</pd:x>
        <pd:y>252</pd:y>
        <config>
            <element ref="pfx3:LrcRedemptionResponse"/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="count($ParseRCMSRespXML/pfx3:RedeemResponse/Error)>0 and not (empty($ParseRCMSRespXML/pfx3:RedeemResponse/Error))">
                    <pfx3:LrcRedemptionResponse>
                        <xsl:attribute name="version">
                            <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/@version"/>
                        </xsl:attribute>
                        <xsl:copy-of select="$Start/root/pfx3:LrcRedemptionRequest/SystemContext"/>
                        <xsl:copy-of select="$Start/root/pfx3:LrcRedemptionRequest/PaymentContext"/>
                        <xsl:choose>
                            <xsl:when test="string-length($ProcessRCMSResponseReturnCode/root/Error/Type)>0">
                                <xsl:for-each select="$ProcessRCMSResponseReturnCode/root/Error">
                                    <Error>
                                        <Type>
                                            <xsl:value-of select="Type"/>
                                        </Type>
                                        <xsl:for-each select="Detail">
                                            <Detail>
                                                <xsl:value-of select="."/>
                                            </Detail>
                                        </xsl:for-each>
                                        <ErrorMessage>
                                            <xsl:value-of select="ErrorMessage"/>
                                        </ErrorMessage>
                                    </Error>
                                </xsl:for-each>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:for-each select="$ParseRCMSRespXML/pfx3:RedeemResponse/Error">
                                    <Error>
                                        <Type>
                                            <xsl:value-of select="Type"/>
                                        </Type>
                                        <xsl:for-each select="Detail">
                                            <Detail>
                                                <xsl:value-of select="."/>
                                            </Detail>
                                        </xsl:for-each>
                                        <ErrorMessage>
                                            <xsl:value-of select="ErrorMessage"/>
                                        </ErrorMessage>
                                    </Error>
                                </xsl:for-each>
                            </xsl:otherwise>
                        </xsl:choose>
                    </pfx3:LrcRedemptionResponse>
                </xsl:when>
                <xsl:otherwise>
                    <pfx3:LrcRedemptionResponse>
                        <xsl:attribute name="version">
                            <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/@version"/>
                        </xsl:attribute>
                        <xsl:copy-of select="$Start/root/pfx3:LrcRedemptionRequest/SystemContext"/>
                        <xsl:copy-of select="$Start/root/pfx3:LrcRedemptionRequest/PaymentContext"/>
                        <xsl:for-each select="$Start/root/pfx3:LrcRedemptionRequest/OrderContext">
                            <OrderContext>
                                <xsl:attribute name="omsOrderId">
                                    <xsl:value-of select="@omsOrderId"/>
                                </xsl:attribute>
                                <xsl:if test="string-length(OrderTime)>0">
                                    <OrderTime>
                                        <xsl:value-of select="OrderTime"/>
                                    </OrderTime>
                                </xsl:if>
                            </OrderContext>
                        </xsl:for-each>
                        <ConsumerTransactionId>
                            <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/ConsumerTransactionId"/>
                        </ConsumerTransactionId>
                        <ConfirmationId>
                            <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RedemptionId"/>
                        </ConfirmationId>
                        <CurrencyCode>
                            <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/CurrencyCode"/>
                        </CurrencyCode>
                        <RequestedAmount>
                            <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RequestedAmount"/>
                        </RequestedAmount>
                        <RedeemedAmount>
                            <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RedeemedAmount"/>
                        </RedeemedAmount>
                        <xsl:if test="$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/BalanceAmount">
                            <NewBalanceAmount>
                                <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/BalanceAmount"/>
                            </NewBalanceAmount>
                        </xsl:if>
                        <xsl:if test="string-length($ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CertificateStatus)>0">
                            <xsl:if test="$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CertificateStatus">
                                <TransactionMetadata>
                                    <xsl:attribute name="type">
                                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/Constants/TransactionMetaData/CERTIFICATE_STATUS"/>
                                    </xsl:attribute>
                                    <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CertificateStatus"/>
                                </TransactionMetadata>
                            </xsl:if>
                        </xsl:if>
                        <xsl:if test="string-length($ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CurrencyCode)>0">
                            <TransactionMetadata>
                                <xsl:attribute name="type">
                                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/Constants/TransactionMetaData/CERTIFICATE_CURRENCY_CODE"/>
                                </xsl:attribute>
                                <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CurrencyCode"/>
                            </TransactionMetadata>
                        </xsl:if>
                        <xsl:if test="string-length($ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CertificateAmount)>0">
                            <TransactionMetadata>
                                <xsl:attribute name="type">
                                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/Constants/TransactionMetaData/CERTIFICATE_AMOUNT"/>
                                </xsl:attribute>
                                <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CertificateAmount"/>
                            </TransactionMetadata>
                        </xsl:if>
                        <xsl:if test="string-length($ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/Expiration)>0">
                            <TransactionMetadata>
                                <xsl:attribute name="type">
                                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/Constants/TransactionMetaData/CERTIFICATE_EXPIRY"/>
                                </xsl:attribute>
                                <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/Expiration"/>
                            </TransactionMetadata>
                        </xsl:if>
                        <xsl:if test="string-length($ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/Concept)>0">
                            <TransactionMetadata>
                                <xsl:attribute name="type">
                                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PaymentService/Constants/TransactionMetaData/CERTIFICATE_CONCEPT"/>
                                </xsl:attribute>
                                <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/Concept"/>
                            </TransactionMetadata>
                        </xsl:if>
                        <xsl:copy-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/TransactionMetadata"/>
                        <FinancialDate>
                            <xsl:value-of select="tib:parse-date($_globalVariables/ns:GlobalVariables/PaymentService/Constants/DateFormat,$ParseRCMSRespXML/pfx3:RedeemResponse/FinancialDate)"/>
                        </FinancialDate>
                        <Tender>
                            <Type>
                                <Code>
                                    <xsl:value-of select="$Start/root/pfx3:LrcRedemptionRequest/Tender/Type/Code"/>
                                </Code>
                                <xsl:choose>
                                    <xsl:when test="string-length($ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CertificateType)>0">
                                        <SubCode>
                                            <xsl:value-of select="upper-case($GetPymtSvcRCMSCertificateTypeSubCodes/ns3:PaymentServiceRCMSCertificateTypeSubCodes/ns3:RCMSCertificateTypeSubCode[ns3:RCMSCertificateType=$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CertificateType][1]/ns3:CertficateTenderSubCode)"/>
                                        </SubCode>
                                    </xsl:when>
                                    <xsl:otherwise>
                                        <SubCode>
                                            <xsl:value-of select="upper-case($ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CertificateType)"/>
                                        </SubCode>
                                    </xsl:otherwise>
                                </xsl:choose>
                            </Type>
                            <CertificateNumber>
                                <xsl:value-of select="$ParseRCMSRespXML/pfx3:RedeemResponse/RewardCertificate/CertificateNumber"/>
                            </CertificateNumber>
                        </Tender>
                    </pfx3:LrcRedemptionResponse>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
