<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:yfc="http://www.sterlingcommerce.com/documentation/YFS/SVCActivation/Request" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types" targetNamespace="http://www.sterlingcommerce.com/documentation/YFS/SVCActivation/Request" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:annotation yfc:fileName="OMSD2_SvcActivationRequest.xsd" yfc:rootElement="WSISVCInfo">
		<xsd:appinfo source="yfc:structure">
			<WSISVCInfo hasAttributes="Y"/>
		</xsd:appinfo>
	</xsd:annotation>
	<xsd:import namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
	<xsd:element name="WSISVCInfo">
		<xsd:complexType>
			<xsd:attribute name="EnvIdentifier" type="yfctype:Text-24" use="required"/>
			<xsd:attribute name="RegIdentifier" type="yfctype:Text-24" use="required"/>
			<xsd:attribute name="SourceSysID" type="yfctype:Text-50" use="required"/>
			<xsd:attribute name="MsgTransactionID" type="yfctype:Text-50" use="required"/>
			<xsd:attribute name="MsgTransactionTime" type="yfctype:TimeStamp" use="required"/>
			<xsd:attribute name="ExtnEnterpriseCode" type="yfctype:String" use="required"/>
			<xsd:attribute name="ExtnEntryType" type="yfctype:Text-40" use="required"/>
			<xsd:attribute name="ExtnActRequesterID" type="yfctype:Text-24" use="required"/>
			<xsd:attribute name="ExtnOrderNo" type="yfctype:Text-40" use="required"/>
			<xsd:attribute name="ExtnSVCInfoKey" type="yfctype:Text-25" use="required"/>
			<xsd:attribute name="ExtnPaymentType" type="yfctype:Text-20" use="required"/>
			<xsd:attribute name="ExtnSVCNo" type="yfctype:Text-20" use="required"/>
			<xsd:attribute name="ExtnSVCCardType" type="yfctype:Text-32" use="required"/>
			<xsd:attribute name="ExtnCardClass" type="yfctype:Text-100" use="optional"/>
			<xsd:attribute name="ExtnCurrencyCode" type="yfctype:Text-20" use="required"/>
			<xsd:attribute name="ExtnDollarValue" type="yfctype:Money" use="required"/>
			<!-- added as part of OMSD3 EPS CR -->
			<xsd:attribute name="ExtnOrderDate" type="yfctype:TimeStamp" use="required"/>
			<xsd:attribute name="ExtnSVCTag" type="yfctype:Text-1000" use="optional"/>
			<xsd:attribute name="ExtnOrgActTranRequestTime" type="yfctype:TimeStamp" use="optional"/>
			<!-- End -->
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
