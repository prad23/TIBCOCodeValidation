<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://paymentservice.wsgc.com/2014/common/PaymentServices/SharedResources/LrcRedemptionDTCMORequest"
	 targetNamespace="http://paymentservice.wsgc.com/2014/common/PaymentServices/SharedResources/LrcRedemptionDTCMORequest"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="LrcRedemptionDTCMORequest">
		<xs:annotation>
			<xs:documentation>Root element for the operation LrcRedemption</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TransactionID" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Identifier of the transaction, generated by the point of sale system</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RequestType" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The Request type.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PaymentType" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Type of Payment LRC </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="LRCOverrideFlag" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The Customer Code for purchasing cards</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Concept" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Concept code to which LRC belongs to</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Channel" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Channel code to which LRC belongs to</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OrderNumber" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Order Number from the request</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CertificateNumber" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>To be used in future.Incase multiple certificate type needs to be supported</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Transactiondate" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Date when transaction was intiated .</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Transactiontime" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Time when transaction was initiated.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OrderTotal" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Total Order Amount </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OrderTaker" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Order Taker like "UserId"</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OrderDate" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Date when Order was done .</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Redeemedamount" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Redemption Amount.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FutureField" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>