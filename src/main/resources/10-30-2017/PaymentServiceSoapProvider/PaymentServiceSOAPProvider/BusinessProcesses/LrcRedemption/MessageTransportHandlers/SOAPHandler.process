                                    </xsl:attribute>
                                    <xsl:if test="string-length($LrcRedemptionCommon/ns4:LrcRedemptionResponse/Tender/Type/Tag[1])>0">
                                        <xsl:attribute name="ExtnPaymentTag">
                                            <xsl:value-of select="tib:concat-sequence-format&#xA;(&#xA;&#x9;for $var in $LrcRedemptionCommon/ns4:LrcRedemptionResponse/Tender/Type/Tag &#xA;&#x9;&#x9;return (&#xA;&#x9;&#x9;&#x9;&#x9;concat($var/@type, '=', $var)   )         &#xA;,&#xA;' ,'&#x9;&#xA;&#xA;)"/>
                                        </xsl:attribute>
                                    </xsl:if>
                                </pfx8:Payment>
                            </xsl:otherwise>
