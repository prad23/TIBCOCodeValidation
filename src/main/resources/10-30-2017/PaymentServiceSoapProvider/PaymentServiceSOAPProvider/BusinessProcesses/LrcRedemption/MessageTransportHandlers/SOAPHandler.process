                                <ns1:class>
                                    <xsl:value-of select="Class"/>
                                </ns1:class>
                                <ns1:processStack>
                                    <xsl:value-of select="ProcessStack"/>
                                </ns1:processStack>
                                <xsl:if test="MsgCode">
