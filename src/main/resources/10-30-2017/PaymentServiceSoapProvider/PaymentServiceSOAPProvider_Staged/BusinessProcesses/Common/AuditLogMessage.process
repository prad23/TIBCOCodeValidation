        <pd:x>169</pd:x>
        <pd:y>71</pd:y>
        <config>
            <variableConfig>/SharedResources/SharedVariables/SV-PaymentServiceAppAuditSwitches.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetPaymentAppAuditSwitchesParams</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetPaymentAppAuditSwitchesParams</pd:from>
        <pd:to>SendLogRequest</pd:to>
        <pd:xpathDescription>AuditOn</pd:xpathDescription>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>($GetPaymentAppAuditSwitchesParams/pfx:PaymentServiceAuditSwitches/pfx:PaymentServiceAuditSwitch[*[local-name()= $_globalVariables/pfx2:GlobalVariables/Constants/COL_INTERFACECODE]=$Start/root/InterfaceCode][*[local-name()=$_globalVariables/pfx2:GlobalVariables/Constants/COL_AUDITMESSAGETYPE]= $Start/root/AuditMessageType]/pfx:AuditMessageFlag=$_globalVariables/pfx2:GlobalVariables/PaymentService/CLE/AuditSwitch) and
$Start/root/payloadTrue="true"</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>SendLogRequest</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetPaymentAppAuditSwitchesParams</pd:from>
        <pd:to>End</pd:to>
        <pd:xpathDescription>AuditOff</pd:xpathDescription>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
