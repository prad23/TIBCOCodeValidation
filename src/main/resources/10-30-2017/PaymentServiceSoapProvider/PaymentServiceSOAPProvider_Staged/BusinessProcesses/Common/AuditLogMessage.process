<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx2="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.wsgc.com/schemas/common/PaymentServiceAuditSwitches.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.ultimosoft.com/CLEH/ExceptionHandler.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://www.ultimosoft.com/CLEH/ExceptionHandler.xsd" schemaLocation="/SharedResources/XMLSchemas/CLE/GlobalErrorObject.xsd"/>
    <pd:name>BusinessProcesses/Common/AuditLogMessage.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="InterfaceCode" type="xsd:string"/>
                    <xsd:element name="AuditMessageType" type="xsd:string"/>
                    <xsd:element name="payloadTrue" type="xsd:boolean"/>
                    <xsd:element ref="ns:LogRequest"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>57</pd:startX>
    <pd:startY>72</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>357</pd:endX>
    <pd:endY>196</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
