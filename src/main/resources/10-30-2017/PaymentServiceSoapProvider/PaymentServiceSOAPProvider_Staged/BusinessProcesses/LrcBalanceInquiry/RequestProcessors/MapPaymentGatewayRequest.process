        </config>
        <pd:inputBindings>
            <Rows>
                <ns6:LrcBalanceInquiryDTCMORequest>
                    <ns6:TransactionID>
                        <xsl:value-of select="concat(tib:right(tib:pad($Start/pfx:LrcBalanceInquiryRequest/SystemContext/TransactionId, 15, 0),15),tib:format-dateTime($_globalVariables/ns2:GlobalVariables/LrcBalanceInquiry/DTCMO/TimeFormat, current-dateTime()))"/>
                    </ns6:TransactionID>
                    <ns6:RequestType>
                        <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/LrcBalanceInquiry/BWTransformation/RequestType"/>
                    </ns6:RequestType>
                    <ns6:PaymentType>
                        <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/LrcBalanceInquiry/BWTransformation/PaymentType"/>
                    </ns6:PaymentType>
                    <ns6:LRCOverrideFlag>
                        <xsl:value-of select="if(string-length($Start/pfx:LrcBalanceInquiryRequest/CanOverrideExpiration) > 0&#xA; and $Start/pfx:LrcBalanceInquiryRequest/CanOverrideExpiration='true')&#xA; then&#xA; $_globalVariables/ns2:GlobalVariables/LrcBalanceInquiry/BWTransformation/IsOverridden&#xA;else&#xA;$_globalVariables/ns2:GlobalVariables/LrcBalanceInquiry/BWTransformation/IsNotOverridden"/>
                    </ns6:LRCOverrideFlag>
                    <xsl:if test="string-length($Start/pfx:LrcBalanceInquiryRequest/OrderContext/@omsOrderId)>0">
                        <ns6:Concept>
                            <xsl:value-of select="$GetPaymentServiceDTCMOConceptsChannels/ns7:PaymentServiceDTCConceptsChannels/ns7:PaymentServiceDTCMOConceptChannel[$Start/pfx:LrcBalanceInquiryRequest/PaymentContext/Concept  = ns7:EPSConceptCode]/ns7:DTCMOConceptCode"/>
                        </ns6:Concept>
                    </xsl:if>
                    <ns6:Channel>
                        <xsl:value-of select="$GetPaymentServiceDTCMOConceptsChannels/ns7:PaymentServiceDTCConceptsChannels/ns7:PaymentServiceDTCMOConceptChannel[$Start/pfx:LrcBalanceInquiryRequest/PaymentContext/Channel  = ns7:EPSChannelCode]/ns7:DTCMOChannelCode"/>
                    </ns6:Channel>
                    <xsl:choose>
                        <xsl:when test="string-length($Start/pfx:LrcBalanceInquiryRequest/OrderContext/@omsOrderId)>0">
                            <ns6:OrderNumber>
                                <xsl:value-of select="$Start/pfx:LrcBalanceInquiryRequest/OrderContext/@omsOrderId"/>
                            </ns6:OrderNumber>
                        </xsl:when>
                        <xsl:otherwise>
                            <ns6:OrderNumber>
                                <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/LrcBalanceInquiry/BWTransformation/DefaultOrderNumber"/>
                            </ns6:OrderNumber>
                        </xsl:otherwise>
                    </xsl:choose>
                    <ns6:CertificateNumber>
                        <xsl:value-of select="$Start/pfx:LrcBalanceInquiryRequest/Tender/CertificateNumber"/>
                    </ns6:CertificateNumber>
                    <ns6:Transactiondate>
                        <xsl:value-of select="tib:format-dateTime($_globalVariables/ns2:GlobalVariables/LrcBalanceInquiry/DTCMO/TransactionDateFormat,$Start/pfx:LrcBalanceInquiryRequest/SystemContext/TransactionTime)"/>
                    </ns6:Transactiondate>
                    <ns6:Transactiontime>
                        <xsl:value-of select="tib:format-dateTime($_globalVariables/ns2:GlobalVariables/LrcBalanceInquiry/DTCMO/TransactionTimeFormat,$Start/pfx:LrcBalanceInquiryRequest/SystemContext/TransactionTime)"/>
                    </ns6:Transactiontime>
                </ns6:LrcBalanceInquiryDTCMORequest>
            </Rows>
        </pd:inputBindings>
    </pd:activity>
