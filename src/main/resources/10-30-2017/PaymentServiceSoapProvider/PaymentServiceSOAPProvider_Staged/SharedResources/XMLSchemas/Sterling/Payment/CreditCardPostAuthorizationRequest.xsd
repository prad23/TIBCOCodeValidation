			<xsd:attribute name="ExtnPaymentTag" type="yfctype:Text-1000" use="optional"/>		
			<xsd:attribute name="OriginalTranRequestTime" type="yfctype:TimeStamp" use="optional"/>
			<!-- end -->
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
