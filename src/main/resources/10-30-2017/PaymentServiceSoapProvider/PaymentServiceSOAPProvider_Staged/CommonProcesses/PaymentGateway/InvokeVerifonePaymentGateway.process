<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx="http://services.wsgc.com/2014/Payment" xmlns:ns="http://www.ultimosoft.com/CLEH/ExceptionHandler.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns3="http://schemas.tibco.com/bw/plugins/xml/5.0/RESTJSONExceptions" xmlns:ns4="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx2="http://www.tibco.com/ns/no_namespace_schema_location/SharedResources/XMLSchemas/Verifone/CreditPreAuthorizationResponse.xsd" xmlns:ns5="http://schemas.tibco.com/bw/pe/plugin/5.0/exceptions" xmlns:ns6="http://xmlns.tibco.com/encodings/mime" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx3="http://ecommerce.wsgc.com/2013/Common" xmlns:ns7="http://schemas.tibco.com/bw/plugins/http/5.0/httpExceptions" xmlns:ns8="http://schemas.tibco.com/bw/plugins/http/5.0/cio" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions">
    <xsd:import namespace="http://services.wsgc.com/2014/Payment" schemaLocation="/SharedResources/XMLSchemas/PaymentServices/2014/Payment/credit-authorization.xsd"/>
    <xsd:import namespace="http://ecommerce.wsgc.com/2013/Common" schemaLocation="/SharedResources/XMLSchemas/PaymentServices/2013/Common/base-types.xsd"/>
    <pd:name>CommonProcesses/PaymentGateway/InvokeVerifonePaymentGateway.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="paymentgatewayrequest" type="xsd:string"/>
                    <xsd:element name="transactionid" type="xsd:string"/>
                    <xsd:element name="provider" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="InterfaceCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="timeout" type="xsd:int"/>
                    <xsd:element name="Transactiontype" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>60</pd:startX>
    <pd:startY>76</pd:startY>
    <pd:returnBindings>
        <root>
            <xsl:if test="string-length($InvokeVerifonePaymentGateway/RequestActivityOutput/asciiContent)>0">
                <PaymentGatewayResponse>
                    <xsl:value-of select="$InvokeVerifonePaymentGateway/RequestActivityOutput/asciiContent"/>
                </PaymentGatewayResponse>
            </xsl:if>
            <xsl:if test="string-length($Process/root/Error/Type) > 0">
                <Error>
                    <Type>
                        <xsl:value-of select="$Process/root/Error/Type"/>
                    </Type>
                    <xsl:for-each select="$Process/root/Error/Detail">
                        <Detail>
                            <xsl:value-of select="."/>
                        </Detail>
                    </xsl:for-each>
                    <ErrorMessage>
                        <xsl:value-of select="$Process/root/Error/ErrorMessage"/>
                    </ErrorMessage>
                </Error>
            </xsl:if>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="PaymentGatewayResponse" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Error" type="pfx3:ErrorType" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>812</pd:endX>
    <pd:endY>75</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <Process>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Error" type="pfx3:ErrorType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Process>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1414174660115</pd:targetNamespace>
    <pd:activity name="Assign-Error">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>497</pd:x>
        <pd:y>223</pd:y>
        <config>
            <variableName>Process</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:choose>
                    <xsl:when test="($_error_InvokeVerifonePaymentGateway/ActivityErrors/ns7:HttpClientException/ns8:statusLine/statusCode=404) or &#xA;(string-length($_error_InvokeVerifonePaymentGateway/ActivityErrors/ns7:HttpCommunicationException/msg)>0)">
                        <Error>
                            <Type>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/REMOTE_UNAVAILABLE_GATEWAY_CONNECTIVITY/Type"/>
                            </Type>
                            <Detail>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/REMOTE_UNAVAILABLE_GATEWAY_CONNECTIVITY/Detail"/>
                            </Detail>
                            <ErrorMessage>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/REMOTE_UNAVAILABLE_GATEWAY_CONNECTIVITY/ErrorMessage"/>
                            </ErrorMessage>
                        </Error>
                    </xsl:when>
                    <xsl:when test="($_error_InvokeVerifonePaymentGateway/ActivityErrors/ns7:HttpClientException/ns8:statusLine/statusCode>500)">
                        <Error>
                            <Type>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/REMOTE_FAILURE/Type"/>
                            </Type>
                            <Detail>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/REMOTE_FAILURE/Detail"/>
                            </Detail>
                            <ErrorMessage>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/REMOTE_FAILURE/ErrorMessage"/>
                            </ErrorMessage>
                        </Error>
                    </xsl:when>
                    <xsl:when test="string-length($_error_InvokeVerifonePaymentGateway/ActivityErrors/ns7:HttpClientException/msg)>0">
                        <Error>
                            <Type>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/HTTP_CLIENT_EXCEPTION/Type"/>
                            </Type>
                            <Detail>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/HTTP_CLIENT_EXCEPTION/Detail"/>
                            </Detail>
                            <ErrorMessage>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/HTTP_CLIENT_EXCEPTION/ErrorMessage"/>
                            </ErrorMessage>
                        </Error>
                    </xsl:when>
                    <xsl:when test="string-length($_error_InvokeVerifonePaymentGateway/ActivityErrors/ns7:HttpServerException/msg)>0">
                        <Error>
                            <Type>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/HTTP_SERVER_EXCEPTION/Type"/>
                            </Type>
                            <Detail>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/HTTP_SERVER_EXCEPTION/Detail"/>
                            </Detail>
                            <ErrorMessage>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/HTTP_SERVER_EXCEPTION/ErrorMessage"/>
                            </ErrorMessage>
                        </Error>
                    </xsl:when>
                    <xsl:when test="string-length($_error_InvokeVerifonePaymentGateway/ActivityErrors/ns5:ActivityTimedOutException/msg)>0">
                        <Error>
                            <Type>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/ACTIVITY_TIMEDOUT_EXCEPTION/Type"/>
                            </Type>
                            <Detail>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/ACTIVITY_TIMEDOUT_EXCEPTION/Detail"/>
                            </Detail>
                            <ErrorMessage>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/ACTIVITY_TIMEDOUT_EXCEPTION/ErrorMessage"/>
                            </ErrorMessage>
                        </Error>
                    </xsl:when>
                    <xsl:when test="$_error/ns1:ErrorReport/Class=$_globalVariables/ns4:GlobalVariables/PaymentService/ValidationConstants/BIND_EXCEPTION_CLASS">
                        <Error>
                            <Type>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/INVALID_REQUEST/Type"/>
                            </Type>
                            <Detail>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/INVALID_REQUEST/Detail"/>
                            </Detail>
                            <ErrorMessage>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/INVALID_REQUEST/ErrorMessage"/>
                            </ErrorMessage>
                        </Error>
                    </xsl:when>
                    <xsl:otherwise>
                        <Error>
                            <Type>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Type"/>
                            </Type>
                            <Detail>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Detail"/>
                            </Detail>
                            <ErrorMessage>
                                <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/ErrorMessage"/>
                            </ErrorMessage>
                        </Error>
                    </xsl:otherwise>
                </xsl:choose>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>210</pd:x>
        <pd:y>223</pd:y>
        <pd:handler>true</pd:handler>
        <config>
