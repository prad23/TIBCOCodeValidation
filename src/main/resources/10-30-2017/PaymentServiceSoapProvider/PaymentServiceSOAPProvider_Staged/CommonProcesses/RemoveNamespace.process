	}
	public void setoutXML(String val) {
		outXML = val;
	}
/****** END SET/GET METHOD, DO NOT MODIFY *****/
	public RemoveNamespaceRemovenamespace() {
	}
	public void invoke() throws Exception {
/* Available Variables: DO NOT MODIFY
	In  : String inXML
	Out : String outXML
* Available Variables: DO NOT MODIFY *****/

	outXML = inXML;
	if( outXML == null ) 
	{ 	
		outXML="";
	}
   	else {
		outXML = outXML.replaceAll(":ns0|ns0:", "");
		outXML = outXML.replaceAll(" xmlns.*\"(&gt;)", "$1");
	}

	}
}
