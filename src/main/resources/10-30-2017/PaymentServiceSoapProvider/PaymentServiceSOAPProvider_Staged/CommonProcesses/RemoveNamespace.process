            <fullsource>package CommonProcesses.RemoveNamespace;
import java.util.*;
import java.io.*;
public class RemoveNamespaceRemovenamespace{
/****** START SET/GET METHOD, DO NOT MODIFY *****/
	protected String inXML = "";
	protected String outXML = "";
	public String getinXML() {
		return inXML;
	}
	public void setinXML(String val) {
		inXML = val;
	}
	public String getoutXML() {
