<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.wsgc.com/schemas/TibcoInternal/GenericEntityDataTranslator.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <xsd:import namespace="http://www.wsgc.com/schemas/TibcoInternal/GenericEntityDataTranslator.xsd" schemaLocation="/SharedResources/XMLSchemas/TibcoInternal/GenericEntityDataTranslator.xsd"/>
    <pd:name>CommonProcesses/GenericEntityTranslator.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="EntityDataTranslatorInput">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="EntityLookupData" type="xsd:string"/>
                    <xsd:element name="FieldDelimiter" type="xsd:string"/>
                    <xsd:element name="RecordDelimiter" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>167</pd:startX>
    <pd:startY>61</pd:startY>
    <pd:returnBindings>
        <pfx:EntityNameValuePairs>
            <xsl:copy-of select="$EntityFields/pfx:EntityNameValuePairs/pfx:EntityNameValue"/>
        </pfx:EntityNameValuePairs>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:EntityNameValuePairs"/>
    <pd:endX>616</pd:endX>
    <pd:endY>61</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <EntityFields ref="pfx:EntityNameValuePairs"/>
        <EntityRecords>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="EntityRecords" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </EntityRecords>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1486500640580</pd:targetNamespace>
    <pd:activity name="SplitEntityRecords">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>298</pd:x>
        <pd:y>57</pd:y>
        <config>
            <variableName>EntityRecords</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:for-each select="tib:tokenize($Start/EntityDataTranslatorInput/EntityLookupData, $_globalVariables/ns:GlobalVariables/LookupTranslation/GenericEntityTranslator/RecordDelimiter)">
                    <EntityRecords>
                        <xsl:value-of select="."/>
                    </EntityRecords>
                </xsl:for-each>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SplitEntityFields">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>462</pd:x>
        <pd:y>58</pd:y>
        <config>
            <variableName>EntityFields</variableName>
        </config>
        <pd:inputBindings>
            <pfx:EntityNameValuePairs>
                <xsl:for-each select="$EntityRecords/root/EntityRecords">
                    <pfx:EntityNameValue>
                        <xsl:variable name="varSplitNV" select="tib:tokenize-allow-empty(current(), $_globalVariables/ns:GlobalVariables/LookupTranslation/GenericEntityTranslator/FieldDelimiter)"/>
                        <pfx:EntityName>
                            <xsl:value-of select="$varSplitNV[position()=1]"/>
                        </pfx:EntityName>
                        <xsl:for-each select="$varSplitNV[position()!=1]">
                            <pfx:EntityValue>
                                <xsl:value-of select="."/>
                            </pfx:EntityValue>
                        </xsl:for-each>
                    </pfx:EntityNameValue>
                </xsl:for-each>
            </pfx:EntityNameValuePairs>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>SplitEntityRecords</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SplitEntityRecords</pd:from>
        <pd:to>SplitEntityFields</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SplitEntityFields</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>