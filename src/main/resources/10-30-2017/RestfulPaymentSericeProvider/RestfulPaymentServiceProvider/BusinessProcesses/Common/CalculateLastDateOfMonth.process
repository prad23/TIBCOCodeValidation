            <fullsource>package BusinessProcesses.Common.Stub.CalculateLastDateOfMonth;
import java.util.*;
import java.io.*;
import java.util.Calendar;


public class CalculateLastDateOfMonthCalculateLastDate{
/****** START SET/GET METHOD, DO NOT MODIFY *****/
	protected int month = 0;
	protected int year = 0;
	protected Date lastDate = null;
	public int getmonth() {
		return month;
	}
	public void setmonth(int val) {
		month = val;
	}
	public int getyear() {
		return year;
	}
	public void setyear(int val) {
		year = val;
	}
	public Date getlastDate() {
		return lastDate;
	}
	public void setlastDate(Date val) {
		lastDate = val;
	}
/****** END SET/GET METHOD, DO NOT MODIFY *****/
	public CalculateLastDateOfMonthCalculateLastDate() {
	}
	public void invoke() throws Exception {
/* Available Variables: DO NOT MODIFY
	In  : int month
	In  : int year
	Out : Date lastDate
* Available Variables: DO NOT MODIFY *****/

  Calendar calendar = new GregorianCalendar(year, month-1,
  5);
  calendar.set(Calendar.DAY_OF_MONTH,
  calendar.getActualMaximum(Calendar.DAY_OF_MONTH));
  lastDate=calendar.getTime();



}
}
