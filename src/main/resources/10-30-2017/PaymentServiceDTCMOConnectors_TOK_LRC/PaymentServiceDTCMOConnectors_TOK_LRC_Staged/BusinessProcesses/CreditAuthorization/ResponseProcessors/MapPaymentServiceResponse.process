<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx21="http://www.tibco.com/ns/no_namespace_schema_location/JSON_GEN_Schemas/TEST.xsd" xmlns:pfx="http://services.wsgc.com/2014/Payment" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.ultimosoft.com/CLEH/ExceptionHandler.xsd" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx1="http://www.tibco.com/schemas/REST-POC/Schemas/Schema.xsd" xmlns:pfx2="http://ecommerce.wsgc.com/2013/Common" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx3="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx4="http://www.wsgc.com/schemas/common/PaymentServiceAuditSwitches.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions">
    <xsd:import namespace="http://services.wsgc.com/2014/Payment" schemaLocation="/SharedResources/XMLSchemas/2014/Payment/credit-authorization.xsd"/>
    <pd:name>BusinessProcesses/CreditAuthorization/ResponseProcessors/MapPaymentServiceResponse.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="IsPLCCTender" type="xsd:boolean"/>
                    <xsd:element name="paymentgatewayresponse" type="xsd:string"/>
                    <xsd:element ref="pfx:CreditCardAuthorizationRequest"/>
                    <xsd:element name="PAN" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TenderSubType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Error" type="pfx2:ErrorType" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>61</pd:startX>
    <pd:startY>100</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="string-length($MapErrorData/pfx:CreditCardAuthorizationResponse/@version)>0">
                <xsl:copy-of select="$MapErrorData/pfx:CreditCardAuthorizationResponse"/>
            </xsl:when>
            <xsl:when test="string-length($UGP_MapPaymentServiceResponse-PLCC-V6/pfx:CreditCardAuthorizationResponse/@version)>0">
                <xsl:copy-of select="$UGP_MapPaymentServiceResponse-PLCC-V6/pfx:CreditCardAuthorizationResponse"/>
            </xsl:when>
            <xsl:otherwise>
                <xsl:copy-of select="$UGP_MapPaymentServiceResponse-CC-V6/pfx:CreditCardAuthorizationResponse"/>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:CreditCardAuthorizationResponse"/>
    <pd:endX>533</pd:endX>
    <pd:endY>88</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1436864167818</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>172</pd:x>
        <pd:y>304</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="GetCLEParams">
        <pd:type>com.tibco.pe.core.GetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.getSharedVariable</pd:resourceType>
        <pd:x>197</pd:x>
        <pd:y>100</pd:y>
        <config>
            <variableConfig>/BusinessProcesses/CreditAuthorization/CLEParams.jobsharedvariable</variableConfig>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="MapErrorData">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>354</pd:x>
        <pd:y>310</pd:y>
        <config>
            <element ref="pfx:CreditCardAuthorizationResponse"/>
        </config>
        <pd:inputBindings>
            <pfx:CreditCardAuthorizationResponse>
                <xsl:attribute xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="version">
                    <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/Authorization/MessageVersion"/>
                </xsl:attribute>
                <SystemContext>
                    <EnvironmentIdentifier>
                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/PaymentService/Constants/SystemContext/EnvironmentIdentifier"/>
                    </EnvironmentIdentifier>
                    <RegionIdentifier>
                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/PaymentService/Constants/SystemContext/RegionIdentifier"/>
                    </RegionIdentifier>
                    <SourceSystemIdentifier>
                        <xsl:value-of select="concat($_globalVariables/pfx3:GlobalVariables/PaymentService/Constants/SystemContext/SourceSystemIdentifierPrefix, BW:getHostName())"/>
                    </SourceSystemIdentifier>
                    <xsl:choose xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                        <xsl:when test="string-length($Start/root/pfx:CreditCardAuthorizationRequest/SystemContext/TransactionId)>0">
                            <TransactionId>
                                <xsl:value-of select="concat($_globalVariables/pfx3:GlobalVariables/Authorization/Constants/TransactionIdPrefix,$Start/root/pfx:CreditCardAuthorizationRequest/SystemContext/TransactionId)"/>
                            </TransactionId>
                        </xsl:when>
                        <xsl:otherwise>
                            <TransactionId>
                                <xsl:value-of select="concat($_globalVariables/pfx3:GlobalVariables/Authorization/Constants/TransactionIdPrefix, tib:format-dateTime($_globalVariables/pfx3:GlobalVariables/PaymentService/Constants/DateTimeFormat, current-dateTime()),   $_processContext/ns:ProcessContext/ProcessId,  $_globalVariables/pfx3:GlobalVariables/ENV   , BW:getHostName())"/>
                            </TransactionId>
                        </xsl:otherwise>
                    </xsl:choose>
                    <TransactionTime>
                        <xsl:value-of select="current-dateTime()"/>
                    </TransactionTime>
                </SystemContext>
                <xsl:copy-of select="$Start/root/pfx:CreditCardAuthorizationRequest/PaymentContext"/>
                <xsl:for-each xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$_error/ns:ErrorReport/Msg">
                    <xsl:choose>
                        <xsl:when test="../Class=$_globalVariables/pfx3:GlobalVariables/PaymentService/ValidationConstants/BIND_EXCEPTION_CLASS">
                            <Error>
                                <Type>
                                    <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/PaymentService/CommonErrors/INVALID_REQUEST/Type"/>
                                </Type>
                                <xsl:if test="string-length($_globalVariables/pfx3:GlobalVariables/PaymentService/CommonErrors/INVALID_REQUEST/Detail)>0">
                                    <Detail>
                                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/PaymentService/CommonErrors/INVALID_REQUEST/Detail"/>
                                    </Detail>
                                </xsl:if>
                                <ErrorMessage>
                                    <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/PaymentService/CommonErrors/INVALID_REQUEST/ErrorMessage"/>
                                </ErrorMessage>
                            </Error>
                        </xsl:when>
                        <xsl:otherwise>
                            <Error>
                                <Type>
                                    <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Type"/>
                                </Type>
                                <xsl:if test="string-length($_globalVariables/pfx3:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Detail)>0">
                                    <Detail>
                                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/Detail"/>
                                    </Detail>
                                </xsl:if>
                                <ErrorMessage>
                                    <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/PaymentService/CommonErrors/SAFE_FAILURE/ErrorMessage"/>
                                </ErrorMessage>
                            </Error>
                        </xsl:otherwise>
                    </xsl:choose>
                </xsl:for-each>
            </pfx:CreditCardAuthorizationResponse>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SendExceptionRequest">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>533</pd:x>
        <pd:y>308</pd:y>
        <config>
            <processName>/CommonProcesses/CLEClientStub/SendExceptionRequest.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns1:ExceptionHandlerRequest>
                <ns1:envCode>
                    <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/ENV"/>
                </ns1:envCode>
                <ns1:appCode>
                    <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/CLE/AppCode"/>
                </ns1:appCode>
                <ns1:interfaceCode>
                    <xsl:value-of select="$GetCLEParams/root/InterfaceCode"/>
                </ns1:interfaceCode>
                <ns1:hostName>
                    <xsl:value-of select="BW:getHostName()"/>
                </ns1:hostName>
                <ns1:timeStamp>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:timeStamp>
                <ns1:message>
                    <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                </ns1:message>
                <xsl:choose xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                    <xsl:when test="string-length($Start/root/pfx:CreditCardAuthorizationRequest/@version) > 0">
                        <ns1:payload>
                            <xsl:value-of select="tib:render-xml($Start/root/pfx:CreditCardAuthorizationRequest)"/>
                        </ns1:payload>
                    </xsl:when>
                    <xsl:otherwise>
                        <ns1:payload>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns1:payload>
                    </xsl:otherwise>
                </xsl:choose>
                <ns1:ErrorLevel>
                    <ns1:errorCode>
                        <xsl:value-of select="concat($MapErrorData/pfx:CreditCardAuthorizationResponse/Error/Type, $_globalVariables/pfx3:GlobalVariables/PaymentService/Constants/ErrorCodeSeparator,&#xA;$MapErrorData/pfx:CreditCardAuthorizationResponse/Error/Detail[1])"/>
                    </ns1:errorCode>
                </ns1:ErrorLevel>
                <ns1:TransactionDetails>
                    <ns1:transactionDomain>
                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/PaymentService/CLE/TransactionDomain"/>
                    </ns1:transactionDomain>
                    <ns1:transactionType>
                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/Authorization/CLE/TransactionType"/>
                    </ns1:transactionType>
                    <xsl:choose xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                        <xsl:when test="string-length($GetCLEParams/root/TransactionId) > 0">
                            <ns1:transactionID>
                                <xsl:value-of select="$GetCLEParams/root/TransactionId"/>
                            </ns1:transactionID>
                        </xsl:when>
                        <xsl:otherwise>
                            <ns1:transactionID>
                                <xsl:value-of select="concat(tib:format-dateTime($_globalVariables/pfx3:GlobalVariables/PaymentService/Constants/DateTimeFormat, current-dateTime()),   $_processContext/ns:ProcessContext/ProcessId,  $_globalVariables/pfx3:GlobalVariables/ENV   , BW:getHostName())"/>
                            </ns1:transactionID>
                        </xsl:otherwise>
                    </xsl:choose>
                </ns1:TransactionDetails>
                <xsl:for-each xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$_error/ns:ErrorReport">
                    <ns1:ErrorReportType>
                        <ns1:stackTrace>
                            <xsl:value-of select="StackTrace"/>
                        </ns1:stackTrace>
                        <ns1:msg>
                            <xsl:value-of select="Msg"/>
                        </ns1:msg>
                        <ns1:fullClass>
                            <xsl:value-of select="FullClass"/>
                        </ns1:fullClass>
                        <ns1:class>
                            <xsl:value-of select="Class"/>
                        </ns1:class>
                        <ns1:processStack>
                            <xsl:value-of select="ProcessStack"/>
                        </ns1:processStack>
                        <xsl:if test="MsgCode">
                            <ns1:msgCode>
                                <xsl:value-of select="MsgCode"/>
                            </ns1:msgCode>
                        </xsl:if>
                        <ns1:data>
                            <xsl:value-of select="Data/*"/>
                        </ns1:data>
                    </ns1:ErrorReportType>
                </xsl:for-each>
            </ns1:ExceptionHandlerRequest>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="UGP_MapPaymentServiceResponse-PLCC-V6">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>358</pd:x>
        <pd:y>35</pd:y>
        <config>
            <processName>/BusinessProcesses/CreditAuthorization/ResponseProcessors/MapPaymentServiceResponse-PLCC.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <paymentgatewayresponse>
                    <xsl:value-of select="$Start/root/paymentgatewayresponse"/>
                </paymentgatewayresponse>
                <xsl:copy-of select="$Start/root/pfx:CreditCardAuthorizationRequest"/>
                <xsl:if test="$Start/root/PAN">
                    <PAN>
                        <xsl:value-of select="$Start/root/PAN"/>
                    </PAN>
                </xsl:if>
                <xsl:if test="$Start/root/TenderSubType">
                    <TenderSubType>
                        <xsl:value-of select="$Start/root/TenderSubType"/>
                    </TenderSubType>
                </xsl:if>
                <xsl:copy-of select="$Start/root/Error"/>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="UGP_MapPaymentServiceResponse-CC-V6">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>353</pd:x>
        <pd:y>157</pd:y>
        <config>
            <processName>/BusinessProcesses/CreditAuthorization/ResponseProcessors/MapPaymentServiceResponse-CC.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <paymentgatewayresponse>
                    <xsl:value-of select="$Start/root/paymentgatewayresponse"/>
                </paymentgatewayresponse>
                <xsl:copy-of select="$Start/root/pfx:CreditCardAuthorizationRequest"/>
                <xsl:if test="$Start/root/PAN">
                    <PAN>
                        <xsl:value-of select="$Start/root/PAN"/>
                    </PAN>
                </xsl:if>
                <xsl:if test="$Start/root/TenderSubType">
                    <TenderSubType>
                        <xsl:value-of select="$Start/root/TenderSubType"/>
                    </TenderSubType>
                </xsl:if>
                <xsl:copy-of select="$Start/root/Error"/>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>UGP_MapPaymentServiceResponse-PLCC-V6</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>UGP_MapPaymentServiceResponse-CC-V6</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>MapErrorData</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>MapErrorData</pd:from>
        <pd:to>SendExceptionRequest</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SendExceptionRequest</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetCLEParams</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetCLEParams</pd:from>
        <pd:to>UGP_MapPaymentServiceResponse-PLCC-V6</pd:to>
        <pd:xpathDescription>PLCC</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/root/IsPLCCTender=$_globalVariables/pfx3:GlobalVariables/PaymentService/Constants/BooleanSuccess</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>GetCLEParams</pd:from>
        <pd:to>UGP_MapPaymentServiceResponse-CC-V6</pd:to>
        <pd:xpathDescription>CC</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>