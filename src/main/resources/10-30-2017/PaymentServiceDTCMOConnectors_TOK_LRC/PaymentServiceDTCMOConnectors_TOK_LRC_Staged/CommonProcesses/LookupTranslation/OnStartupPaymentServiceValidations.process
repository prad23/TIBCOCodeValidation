<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:ns="http://www.tibco.com/schemas/Phase1BWTemplate/SharedResources/SchemaDefinitions/Common/Schema.xsd" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns3="http://www.wsgc.com/schemas/common/PaymentServiceValidations.xsd" xmlns:pfx2="http://www.tibco.com/schemas/Phase1BWTemplate/SharedResources/SchemaDefinitions/Common/Schema.xsd2" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx3="http://www.wsgc.com/schemas/common/ItemConcepts.xsd" xmlns:pfx4="http://www.wsgc.com/schemas/common/RMSLocations.xsd" xmlns:pfx5="http://www.wsgc.com/schemas/common/ConceptFilters.xsd" xmlns:pfx6="http://www.wsgc.com/schemas/common/CLEFlags.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx7="http://www.wsgc.com/schemas/common/PaymentServiceDTCMOConceptsChannels.xsd" xmlns:pfx8="http://www.wsgc.com/schemas/common/PaymentServiceValidationRules.xsd">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/Phase1BWTemplate/SharedResources/SchemaDefinitions/Common/Schema.xsd" schemaLocation="/SharedResources/SchemaDefinitions/Common/LookupTranslations.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.wsgc.com/schemas/common/ItemConcepts.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/ItemConcepts.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.wsgc.com/schemas/common/ConceptFilters.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/ConceptFilters.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.wsgc.com/schemas/common/RMSLocations.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.wsgc.com/schemas/common/CLEFlags.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/CLEFlags.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.wsgc.com/schemas/common/PaymentServiceValidationRules.xsd" schemaLocation="/SharedResources/XMLSchemas/Common/PaymentServiceValidationRules.xsd"/>
    <pd:name>CommonProcesses/LookupTranslation/OnStartupPaymentServiceValidations.process</pd:name>
    <pd:startName>onStartup</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="onStartup">
        <pd:type>com.tibco.pe.core.OnStartupEventSource</pd:type>
        <pd:resourceType>ae.activities.onStartup</pd:resourceType>
        <pd:x>80</pd:x>
        <pd:y>99</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>944</pd:endX>
    <pd:endY>98</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1200594477234</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>407</pd:x>
        <pd:y>10</pd:y>
        <pd:width>327</pd:width>
        <pd:height>167</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$ParseValidationRules/pfx8:PaymentServiceValidationRules/pfx8:PaymentServiceValidationRule</pd:over>
            <pd:iterationElementSlot>FileInfo</pd:iterationElementSlot>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:outSlot>XsltFiles</pd:outSlot>
            <pd:activityOutputName>Read File</pd:activityOutputName>
            <pd:accumulateOutput>true</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Read File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Read File">
            <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
            <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
            <pd:x>557</pd:x>
            <pd:y>102</pd:y>
            <config>
                <encoding>text</encoding>
            </config>
            <pd:inputBindings>
                <pfx:ReadActivityInputClass>
                    <fileName>
                        <xsl:value-of select="concat($_globalVariables/ns1:GlobalVariables/CommonFilePath/PaymentServiceValidationXSLTsPath,$FileInfo/pfx8:PaymentServiceValidationRule,$_globalVariables/ns1:GlobalVariables/CommonFilePath/PaymentServiceValidationRuleExtension)"/>
                    </fileName>
                    <encoding>
                        <xsl:value-of select="&quot;UTF-8&quot;"/>
                    </encoding>
                </pfx:ReadActivityInputClass>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="SetVariable">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>829</pd:x>
        <pd:y>103</pd:y>
        <config>
            <variableConfig>/SharedResources/SharedVariables/SV-PaymentServiceValidations.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings>
            <ns3:PaymentServiceValidations>
                <xsl:for-each select="$XsltFiles/AccumulatedOutput/ns2:ReadActivityOutputTextClass">
                    <ns3:PaymentServiceValidation>
                        <ns3:RuleName>
                            <xsl:value-of select="substring-before(fileInfo/fileName, &quot;.&quot;)"/>
                        </ns3:RuleName>
                        <ns3:RuleContent>
                            <xsl:value-of select="fileContent/textContent"/>
                        </ns3:RuleContent>
                    </ns3:PaymentServiceValidation>
                </xsl:for-each>
            </ns3:PaymentServiceValidations>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ParseValidationRules">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>304</pd:x>
        <pd:y>100</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>false</validateOutput>
            <term ref="pfx8:PaymentServiceValidationRules"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$ReadValidationRules/ns2:ReadActivityOutputTextClass/fileContent/textContent"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ReadValidationRules">
        <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
        <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
        <pd:x>182</pd:x>
        <pd:y>100</pd:y>
        <config>
            <encoding>text</encoding>
        </config>
        <pd:inputBindings>
            <pfx:ReadActivityInputClass>
                <fileName>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/CommonFilePath/PaymentServiceValidationRules"/>
                </fileName>
            </pfx:ReadActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>SetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SetVariable</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>onStartup</pd:from>
        <pd:to>ReadValidationRules</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ReadValidationRules</pd:from>
        <pd:to>ParseValidationRules</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ParseValidationRules</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>